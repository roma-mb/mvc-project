{
    "autoload": {
        "psr-4": {
            "APP\\": "src/"
        },
        "files": [
            "src/Helpers/functions.php"
        ]
    },
    "require": {
        "doctrine/orm": "*",
        "doctrine/cache": "^2.1",
        "symfony/cache": "^5.4",
        "friendsofphp/php-cs-fixer": "^3.4"
    },
    "scripts": {
        "cs":  "vendor/bin/php-cs-fixer list-files --config=.php-cs-fixer.php",
        "cs-fixer":  "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.php --allow-risky=yes"
    }
}
